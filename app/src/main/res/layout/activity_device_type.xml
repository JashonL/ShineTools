<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <include layout="@layout/toobar_title" />


    <TextView
        android:id="@+id/tv_select"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:text="@string/android_key1322"
        android:textColor="@color/color_text_66"
        android:textSize="@dimen/size_content_sp_14" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!--max mac mod mid-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp_10"
                android:background="@drawable/shape_white_corner"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_max"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center"
                    android:padding="@dimen/dp_10"
                    android:text="MAX/MAC/MOD/MID"
                    android:textColor="@color/color_text_99"
                    android:textSize="@dimen/size_content_sp_14" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:background="@color/gray_eeeeeee" />

                <LinearLayout
                    android:id="@+id/ll_type_max"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_max"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="MAX/MAC/MOD/MID"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_type_230"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_230"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="MAX 230KTL3 HV"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_type_max_x"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_max_x"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="MAX-X"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_type_tl3_xh"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_tl3_xh"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="TL3-XH"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


            </LinearLayout>


            <!--min/mic-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp_10"
                android:background="@drawable/shape_white_corner"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_min"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center"
                    android:padding="@dimen/dp_10"
                    android:text="MIN/MIC"
                    android:textColor="@color/color_text_99"
                    android:textSize="@dimen/size_content_sp_14" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_1"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_marginEnd="@dimen/dp_10"
                    android:background="@color/gray_eeeeeee" />

                <LinearLayout
                    android:id="@+id/ll_type_tl_x"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_tl_x"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="TL-X/TL-E"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_tl_xh"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_tl_xh"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="TL-XH"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_type_tlx_us"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_tlx_us"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="TL-XH-US"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


            </LinearLayout>


            <!---s/mtl-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp_10"
                android:background="@drawable/shape_white_corner"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_type_olb"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_olb"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="-S/MTL-S/TL3-S..."
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/device_old" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


            </LinearLayout>


            <!--sph-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp_10"
                android:background="@drawable/shape_white_corner"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_type_sph"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_type_sph"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/dp_45"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp_10"
                        android:text="SPH/SPA"
                        android:textColor="@color/color_text_33"
                        android:textSize="@dimen/size_content_sp_14" />


                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:src="@drawable/more" />

                </LinearLayout>


            </LinearLayout>

        </LinearLayout>

    </ScrollView>


</LinearLayout>