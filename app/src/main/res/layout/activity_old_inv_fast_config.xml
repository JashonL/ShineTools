<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white1"
    android:orientation="vertical">
    <include layout="@layout/header_view_title_v3" android:id="@+id/headerView"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_15"
                android:background="#eaeaea"
                app:layout_constraintTop_toTopOf="parent"
                />
            <TextView
                android:id="@+id/tvTitle1"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_40"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/iphone_background"
                android:text="@string/m国家安规"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_10"
                android:textColor="#009cff"
                android:textSize="@dimen/size_content_sp_14"
                />
            <CheckBox
                android:id="@+id/checkBox1"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                app:layout_constraintTop_toTopOf="@id/tvTitle1"
                app:layout_constraintBottom_toBottomOf="@+id/tvTitle1"
                app:layout_constraintRight_toRightOf="@id/tvTitle1"
                android:layout_marginRight="@dimen/dp_10"
                android:theme="@style/MyCheckBox"
                android:clickable="false"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/btnSelect1Type"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="@string/PutinActivity_search"
                android:gravity="center"
                app:autoSizeTextType="uniform"
                app:autoSizeMinTextSize="@dimen/sp_6"
                app:autoSizeMaxTextSize="@dimen/size_content_note"
                android:textSize="@dimen/size_content_note"
                android:textColor="@color/max_main_text_content"
                android:background="@drawable/bg_gray_stroke"
                app:layout_constraintTop_toTopOf="@+id/btnSelectModel"
                app:layout_constraintBottom_toBottomOf="@+id/btnSelectModel"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/btnSelect1Mech"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/btnSelect1Mech"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:text="@string/PutinActivity_search"
                android:gravity="center"
                app:autoSizeTextType="uniform"
                app:autoSizeMinTextSize="@dimen/sp_6"
                app:autoSizeMaxTextSize="@dimen/size_content_note"
                android:textSize="@dimen/size_content_note"
                android:textColor="@color/max_main_text_content"
                android:background="@drawable/bg_gray_stroke"
                app:layout_constraintTop_toTopOf="@+id/btnSelectModel"
                app:layout_constraintBottom_toBottomOf="@+id/btnSelectModel"
                app:layout_constraintLeft_toRightOf="@+id/btnSelect1Type"
                app:layout_constraintRight_toLeftOf="@+id/btnSelectModel"
                />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/btnSelectModel"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_35"
                android:text="@string/PutinActivity_search"
                android:gravity="center"
                app:autoSizeTextType="uniform"
                app:autoSizeMinTextSize="@dimen/sp_6"
                app:autoSizeMaxTextSize="@dimen/size_content_note"
                android:textSize="@dimen/size_content_note"
                android:textColor="@color/max_main_text_content"
                android:background="@drawable/bg_gray_stroke"
                app:layout_constraintTop_toBottomOf="@+id/tvTitle1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/btnSelect1Mech"
                android:layout_marginTop="@dimen/dp_25"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:visibility="gone"
                />
            <Button
                android:id="@+id/btnSelect1"
                android:layout_width="@dimen/dp_250"
                android:layout_height="@dimen/dp_35"
                android:text="@string/PutinActivity_search"
                android:gravity="center"
                android:textSize="@dimen/size_content_sp_14"
                android:textColor="@color/max_main_text_content"
                android:background="@drawable/bg_gray_stroke"
                app:layout_constraintTop_toBottomOf="@+id/tvTitle1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/tvTitle2"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_40"
                app:layout_constraintTop_toBottomOf="@+id/btnSelect1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:background="@color/iphone_background"
                android:text="@string/mlocal逆变器时间"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_10"
                android:textColor="#009cff"
                android:textSize="@dimen/size_content_sp_14"
                />
            <CheckBox
                android:id="@+id/checkBox2"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                app:layout_constraintTop_toTopOf="@id/tvTitle2"
                app:layout_constraintBottom_toBottomOf="@+id/tvTitle2"
                app:layout_constraintRight_toRightOf="@id/tvTitle2"
                android:layout_marginRight="@dimen/dp_10"
                android:theme="@style/MyCheckBox"
                android:clickable="false"
                />
            <Button
                android:id="@+id/btnSelect2"
                android:layout_width="@dimen/dp_250"
                android:layout_height="@dimen/dp_35"
                android:text="@string/PutinActivity_search"
                android:gravity="center"
                android:textSize="@dimen/size_content_sp_14"
                android:textColor="@color/max_main_text_content"
                android:background="@drawable/bg_gray_stroke"
                app:layout_constraintTop_toBottomOf="@+id/tvTitle2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/tvTitle3"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_40"
                app:layout_constraintTop_toBottomOf="@+id/btnSelect2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:background="@color/iphone_background"
                android:text="@string/mLCD语言"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_10"
                android:textColor="#009cff"
                android:textSize="@dimen/size_content_sp_14"
                />
            <CheckBox
                android:id="@+id/checkBox3"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                app:layout_constraintTop_toTopOf="@id/tvTitle3"
                app:layout_constraintBottom_toBottomOf="@+id/tvTitle3"
                app:layout_constraintRight_toRightOf="@id/tvTitle3"
                android:layout_marginRight="@dimen/dp_10"
                android:theme="@style/MyCheckBox"
                android:clickable="false"
                />
            <Button
                android:id="@+id/btnSelect3"
                android:layout_width="@dimen/dp_250"
                android:layout_height="@dimen/dp_35"
                android:text="@string/PutinActivity_search"
                android:gravity="center"
                android:textSize="@dimen/size_content_sp_14"
                android:textColor="@color/max_main_text_content"
                android:background="@drawable/bg_gray_stroke"
                app:layout_constraintTop_toBottomOf="@+id/tvTitle3"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/tvTitle4"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_40"
                app:layout_constraintTop_toBottomOf="@+id/btnSelect3"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:background="@color/iphone_background"
                android:text="@string/m423通信地址"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_10"
                android:textColor="#009cff"
                android:textSize="@dimen/size_content_sp_14"
                />
            <CheckBox
                android:id="@+id/checkBox4"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                app:layout_constraintTop_toTopOf="@id/tvTitle4"
                app:layout_constraintBottom_toBottomOf="@+id/tvTitle4"
                app:layout_constraintRight_toRightOf="@id/tvTitle4"
                android:layout_marginRight="@dimen/dp_10"
                android:theme="@style/MyCheckBox"
                android:clickable="false"
                />
            <Button
                android:id="@+id/btnSelect4"
                android:layout_width="@dimen/dp_250"
                android:layout_height="@dimen/dp_35"
                android:text="@string/PutinActivity_search"
                android:gravity="center"
                android:textSize="@dimen/size_content_sp_14"
                android:textColor="@color/max_main_text_content"
                android:background="@drawable/bg_gray_stroke"
                app:layout_constraintTop_toBottomOf="@+id/tvTitle4"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/tvTitle5"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_40"
                app:layout_constraintTop_toBottomOf="@+id/btnSelect4"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="@dimen/dp_25"
                android:background="@color/iphone_background"
                android:text="@string/m自动测试"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/dp_10"
                android:textColor="#009cff"
                android:textSize="@dimen/size_content_sp_14"
                />
            <CheckBox
                android:id="@+id/checkBox5"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_30"
                app:layout_constraintTop_toTopOf="@id/tvTitle5"
                app:layout_constraintBottom_toBottomOf="@+id/tvTitle5"
                app:layout_constraintRight_toRightOf="@id/tvTitle5"
                android:layout_marginRight="@dimen/dp_10"
                android:theme="@style/MyCheckBox"
                android:clickable="false"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</LinearLayout>
