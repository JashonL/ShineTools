<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white1">
    <include layout="@layout/header_view_title_v3" android:id="@+id/headerView"/>
    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="end"
        app:constraint_referenced_ids="tvConfigWifiNote,tvCodeNote,tvVolNote,tvOutputNote,tvTimeNote,tvPhoneNote" />
    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvConfigWifiNote"
        />
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/tvConfigWifiNote"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        android:maxWidth="@dimen/dp_50"
        app:layout_constraintTop_toBottomOf="@+id/headerView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:autoSizeTextType="uniform"
        app:autoSizeMaxTextSize="@dimen/size_title_sp_14"
        app:autoSizeMinTextSize="@dimen/sp_6"
        android:maxLines="2"
        android:text="@string/android_key3039"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_title_sp_14"
        />
    <View
        android:id="@+id/viewConfig"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvConfigWifiNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvConfigWifiNote"
        app:layout_constraintLeft_toLeftOf="@+id/barrier"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        />
    <TextView
        android:id="@+id/tvConfigWifi"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/viewConfig"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvConfigWifiNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvConfigWifiNote"
        android:layout_marginRight="@dimen/dp_10"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <ImageView
        android:layout_width="@dimen/dp_6"
        android:layout_height="0dp"
        android:src="@drawable/max_right"
        app:layout_constraintRight_toRightOf="@+id/tvConfigWifi"
        app:layout_constraintTop_toTopOf="@+id/tvConfigWifi"
        app:layout_constraintBottom_toBottomOf="@+id/tvConfigWifi"
        />


    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvDynamometer"
        />
    <TextView
        android:id="@+id/tvDynamometer"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/tvConfigWifi"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/android_key256"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_title_sp_14"
        />
    <View
        android:id="@+id/viewDyNamometer"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvDynamometer"
        app:layout_constraintBottom_toBottomOf="@+id/tvDynamometer"
        app:layout_constraintLeft_toLeftOf="@+id/barrier"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        />
    <TextView
        android:id="@+id/tvDyNamometer1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/viewDyNamometer"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvDynamometer"
        app:layout_constraintBottom_toBottomOf="@+id/tvDynamometer"
        android:layout_marginRight="@dimen/dp_10"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <ImageView
        android:layout_width="@dimen/dp_6"
        android:layout_height="0dp"
        android:src="@drawable/max_right"
        app:layout_constraintRight_toRightOf="@+id/tvDyNamometer1"
        app:layout_constraintTop_toTopOf="@+id/tvDyNamometer1"
        app:layout_constraintBottom_toBottomOf="@+id/tvDyNamometer1"
        />




    <View
        android:id="@+id/vCode"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvCodeNote"
        />
    <TextView
        android:id="@+id/tvCodeNote"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/tvDyNamometer1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="@string/android_key3040"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <View
        android:id="@+id/viewV"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvCodeNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvCodeNote"
        app:layout_constraintLeft_toLeftOf="@+id/barrier"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        />
    <TextView
        android:id="@+id/tvCode"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/viewV"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvCodeNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvCodeNote"
        android:layout_marginRight="@dimen/dp_10"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <ImageView
        android:id="@+id/ivCodeNext"
        android:layout_width="@dimen/dp_6"
        android:layout_height="0dp"
        android:src="@drawable/max_right"
        app:layout_constraintRight_toRightOf="@+id/tvCode"
        app:layout_constraintTop_toTopOf="@+id/tvCode"
        app:layout_constraintBottom_toBottomOf="@+id/tvCode"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvVolNote"
        />
    <TextView
        android:id="@+id/tvVolNote"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/tvCodeNote"
        app:layout_constraintLeft_toLeftOf="@+id/tvCodeNote"
        android:text="@string/android_key3041"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_title_sp_14"
        />
    <TextView
        android:id="@+id/tvVol"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/v_vol"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="@dimen/dp_10"
        app:layout_constraintTop_toTopOf="@+id/tvVolNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvVolNote"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <View
        android:id="@+id/v_vol"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvVolNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvVolNote"
        app:layout_constraintLeft_toLeftOf="@+id/barrier"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvOutputNote"
        />
    <TextView
        android:id="@+id/tvOutputNote"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/tvVolNote"
        app:layout_constraintLeft_toLeftOf="@+id/tvCodeNote"
        android:text="@string/android_key3043"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_title_sp_14"
        />
    <TextView
        android:id="@+id/tvOutput"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/vOutPut"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="@dimen/dp_10"
        app:layout_constraintTop_toTopOf="@+id/tvOutputNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvOutputNote"
        android:text="@string/android_key3045"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <View
        android:id="@+id/vOutPut"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvOutputNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvOutputNote"
        app:layout_constraintLeft_toLeftOf="@+id/barrier"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        android:layout_marginLeft="@dimen/dp_10"
        />


<!--=======================================================-->

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvEms"
        />
    <TextView
        android:id="@+id/tvEms"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/tvOutput"
        app:layout_constraintLeft_toLeftOf="@+id/tvCodeNote"
        android:text="@string/EMS"
        android:drawableEnd="@drawable/icon_ps"
        android:paddingEnd="@dimen/dp_5"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_title_sp_14"
        />
    <TextView
        android:id="@+id/tvEmsInput"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/v_ems"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="@dimen/dp_10"
        app:layout_constraintTop_toTopOf="@+id/tvEms"
        app:layout_constraintBottom_toBottomOf="@+id/tvEms"
        android:text="@string/mSpf5k_电池优先"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <View
        android:id="@+id/v_ems"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvEms"
        app:layout_constraintBottom_toBottomOf="@+id/tvEms"
        app:layout_constraintLeft_toLeftOf="@+id/barrier"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        android:layout_marginLeft="@dimen/dp_10"
        />

    <!--=======================================================-->

    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvTimeNote"
        />
    <TextView
        android:id="@+id/tvTimeNote"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        app:layout_constraintTop_toBottomOf="@+id/tvEmsInput"
        app:layout_constraintLeft_toLeftOf="@+id/tvCodeNote"
        android:text="@string/m4"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_title_sp_14"
        />
    <TextView
        android:id="@+id/tvTime"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/v_time"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="@dimen/dp_10"
        app:layout_constraintTop_toTopOf="@+id/tvTimeNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvTimeNote"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <ImageView
        android:layout_width="@dimen/dp_6"
        android:layout_height="0dp"
        app:layout_constraintRight_toRightOf="@+id/tvTime"
        app:layout_constraintTop_toTopOf="@+id/tvTime"
        app:layout_constraintBottom_toBottomOf="@+id/tvTime"
        />
    <View
        android:id="@+id/v_time"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvTimeNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvTimeNote"
        app:layout_constraintLeft_toLeftOf="@+id/barrier"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvPhoneNote"
        />
    <View
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:visibility="gone"
        android:background="@color/grid_bg_white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/tvPhoneNote"
        />
    <TextView
        android:id="@+id/tvPhoneNote"
        android:layout_width="wrap_content"
        android:minWidth="@dimen/dp_100"
        android:layout_height="@dimen/dp_45"
        android:paddingLeft="@dimen/dp_10"
        android:visibility="gone"
        app:layout_constraintTop_toBottomOf="@+id/tvTimeNote"
        app:layout_constraintLeft_toLeftOf="@+id/tvCodeNote"
        android:text="Use phone time"
        android:gravity="center_vertical"
        android:textColor="@color/content_bg_white"
        android:textSize="@dimen/size_title_sp_14"
        />
    <TextView
        android:id="@+id/tvPhone"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="@+id/tvCode"
        app:layout_constraintRight_toRightOf="@+id/tvCode"
        app:layout_constraintTop_toTopOf="@+id/tvPhoneNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvPhoneNote"
        android:gravity="center"
        android:textColor="@color/title_bg_white"
        android:textSize="@dimen/size_content_sp_14"
        />
    <Switch
        android:id="@+id/swTurn"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintRight_toRightOf="@+id/tvPhone"
        app:layout_constraintTop_toTopOf="@+id/tvPhone"
        app:layout_constraintBottom_toBottomOf="@+id/tvPhone"
        android:checked="false"
        android:track="@drawable/switch_custom_track_selector"
        android:thumb="@drawable/switch_custom_thumb_white"
        android:switchMinWidth="@dimen/dp_45"
        />
    <View
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:visibility="gone"
        android:background="@color/content_bg_white"
        app:layout_constraintTop_toTopOf="@+id/tvPhoneNote"
        app:layout_constraintBottom_toBottomOf="@+id/tvPhoneNote"
        app:layout_constraintLeft_toLeftOf="@+id/viewV"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_5"
        />
    <Button
        android:id="@+id/btnOK"
        android:layout_width="@dimen/dp_250"
        android:layout_height="@dimen/dp_45"
        android:background="@drawable/update_progress"
        app:layout_constraintTop_toBottomOf="@+id/tvPhone"
        android:layout_marginTop="@dimen/dp_40"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:text="@string/m297设置"
        android:textColor="@color/white1"
        android:textSize="@dimen/size_title_sp_14"
        android:layout_gravity="center"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
